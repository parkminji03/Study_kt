#11021
n=int(input())

for i in range(n):
    a,b=input().split(" ")
    a=int(a)
    b=int(b)
    print(f'Case #{i+1}: {a+b}')
  
#11022

n=int(input())

for i in range(n):
    a,b=input().split(" ")
    a=int(a)
    b=int(b)
    print(f'Case #{i+1}: {a} + {b} = {a+b}')
 
#2438
a=int(input())

for i in range(1,a+1):
    print("*"*i)

#2439
a=int(input())

for i in range(1,a+1):
    print(" "*(a-i),end="")
    print("*"*i)

#10871
x,n=map(int,input().split())
a=[]
a=list(map(int,input().split()))
for i in range (x):
    if a[i]<n:
        print(a[i],end=" ")

