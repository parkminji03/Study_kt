#2739
a=int(input())

for i in range(1,10):
    print(a,"*",i,"=",a*i)
    
#10950
t=int(input())

for i in range(1,t+1):
    a,b=input().split(" ")
    a=int(a)
    b=int(b)
    print(a+b)

#8393
a=int(input())
sum=0

for i in range(1,a+1):
    sum+=i

print(sum)

#15552(*)
import sys

t=int(input())
for i in range(1,t+1):
    a,b=sys.stdin.readline().split()
    a=int(a)
    b=int(b)
    print(a+b)

